{
  "name": "reactsmall",
  "version": "0.0.1",
  "description": "small react phonegap project example",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development rm -rf ./www/css/index.* && ./node_modules/.bin/webpack --config webpack.dev.config.js && node server",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p",
    "build-dev": "NODE_ENV=development ./node_modules/.bin/webpack",
    "test": "BABEL_JEST_STAGE=0 rm -rf ./www/css/index.* && ./node_modules/.bin/jest --coverage",
    "mochatest": "NODE_ENV=production ./node_modules/mocha/bin/mocha --compilers js:babel-core/register,css:css-null-compiler.js --recursive --require ./test/setup.js",
    "mochatest:watch": "npm run mochatest -- --watch",
    "watch:test": "nodemon -w __tests__ -w rm -rf ./www/css/index.* && ./src -e js -x npm test"
  },
  "author": "tommy-carlos williams <tommy@devgeeks.org>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.7.0",
    "classnames": "^2.1.3",
    "history": "^1.9.0",
    "offline-js": "^0.7.11",
    "react": "^0.14.0",
    "react-addons-css-transition-group": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-icons": "^0.1.4",
    "react-redux": "^3.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.1",
    "redux-devtools": "^2.1.2",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.21",
    "babel-core": "^5.8.20",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.20",
    "chai": "^3.4.1",
    "css-loader": "^0.19.0",
    "expect": "^1.9.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "jest": "^0.1.40",
    "jest-cli": "^0.5.0",
    "jsdom": "^7.1.0",
    "less-loader": "^2.2.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.4.1",
    "react-addons-test-utils": "^0.14.3",
    "react-tools": "^0.13.3",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^1.1.2",
    "sinon": "^1.16.1",
    "style-loader": "^0.12.3",
    "uglify-js": "^2.4.24",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "jest": {
    "testFileExtensions": [
      "js"
    ],
    "scriptPreprocessor": "<rootDir>/jest-preprocessor",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/timeout-transition-group",
      "<rootDir>/node_modules/expect",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/sinon",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/redux-thunk",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools",
      "<rootDir>/node_modules/react-devtools"
    ],
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
